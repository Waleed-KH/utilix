{
	"name": "@utilix/monorepo",
	"version": "0.0.0",
	"private": true,
	"packageManager": "pnpm@8.10.5",
	"type": "module",
	"scripts": {
		"build": "pnpm -r run build",
		"test": "vitest",
		"tsc:build": "pnpm --filter utilix tsc:build",
		"typecheck": "pnpm tsc:build && tsc --noEmit --project tsconfig.node.json && vue-tsc --noEmit -p tsconfig.docs.json",
		"lint": "pnpm tsc:build && eslint --cache .",
		"lint:fix": "pnpm tsc:build && pnpm lint --fix",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:preview": "vitepress preview docs"
	},
	"keywords": [],
	"devDependencies": {
		"@tsconfig/node20": "^20.1.2",
		"@types/eslint": "^8.44.7",
		"@types/node": "20.9.3",
		"@typescript-eslint/eslint-plugin": "^6.12.0",
		"@typescript-eslint/parser": "^6.12.0",
		"@vue/tsconfig": "^0.4.0",
		"changelogithub": "^0.13.2",
		"eslint": "^8.54.0",
		"typescript": "^5.3.2",
		"vite": "^5.0.2",
		"vitepress": "1.0.0-rc.29",
		"vitest": "^0.34.6",
		"vue": "^3.3.8",
		"vue-tsc": "^1.8.22"
	},
	"author": "",
	"license": "MIT"
}
