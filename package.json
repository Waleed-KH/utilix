{
	"name": "@utilix/monorepo",
	"version": "0.0.0",
	"private": true,
	"packageManager": "pnpm@8.14.3",
	"type": "module",
	"scripts": {
		"build": "pnpm -r run build",
		"test": "vitest",
		"tsc:build": "tsc --build && pnpm --filter utilix tsc:build",
		"typecheck": "pnpm tsc:build && tsc --noEmit --project tsconfig.node.json && vue-tsc --noEmit -p docs/tsconfig.json",
		"lint": "pnpm tsc:build && eslint --cache .",
		"lint:fix": "pnpm tsc:build && pnpm lint --fix",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:preview": "vitepress preview docs"
	},
	"keywords": [],
	"devDependencies": {
		"@tsconfig/node20": "^20.1.2",
		"@types/eslint": "^8.56.2",
		"@types/node": "20.11.5",
		"@typescript-eslint/eslint-plugin": "^6.19.1",
		"@typescript-eslint/parser": "^6.19.1",
		"@vue/tsconfig": "^0.5.1",
		"changelogithub": "^0.13.3",
		"eslint": "^8.56.0",
		"tsutils": "^3.21.0",
		"typescript": "^5.3.3",
		"vite": "^5.0.12",
		"vite-plugin-inspect": "^0.8.2",
		"vitepress": "1.0.0-rc.40",
		"vitest": "^1.2.1",
		"vue": "^3.4.15",
		"vue-tsc": "^1.8.27"
	},
	"author": "",
	"license": "MIT"
}
